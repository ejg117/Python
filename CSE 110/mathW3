num1 = 4
num2 = 3
addition = num1 + num2
subtraction = num1 - num2
multiplication = num1 * num2
division = num1 / num2
division_int = num1 // num2
remainder = num1 % num2
exponent = num1 ** num2

print(addition)
print(subtraction)
print(multiplication)
print(division)
print(division_int)
print(remainder)
print(exponent)

